<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity ProjectLand.
    -->
    <changeSet id="20220712162114-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="survey_id"
                                 baseTableName="landsys_project_land"
                                 constraintName="fk_landsys_project_land__survey_id"
                                 referencedColumnNames="id"
                                 referencedTableName="landsys_survey"/>

        <addForeignKeyConstraint baseColumnNames="land_id"
                                 baseTableName="landsys_project_land"
                                 constraintName="fk_landsys_project_land__land_id"
